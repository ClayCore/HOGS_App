	nav.links
		-
			var list = [['Home', '/'], ['Server Stats', '/server_stats'], ['Ban Appeal', '/ban_appeal'],
						['Feedback', '/feedback'], ['Contact', '/contact']]
		each anchor in list
			a(href=anchor[1])
				span= anchor[0]
