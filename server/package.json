{
    "name": "hightower-ogs-website-server",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "^15.0.0",
        "yarn": "^1.12.4"
    },
    "browserslist": [
        "since 2019-12"
    ],
    "lint-staged": {
        "*.{tsx, ts, js, scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "scripts": {
        "install": "yarn install --ignore-scripts",
        "build:server": "tsc",
        "listen": "ts-node -r tsconfig-paths/register src/server.tsx",
        "build": "yarn build:server",
        "serve": "yarn build:server && yarn listen",
        "start": "yarn build:server && yarn listen"
    },
    "dependencies": {
        "express": "^4.17.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0",
        "typescript": "^4.2.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/node": "^14.14.14",
        "@types/react": "^16.9.43",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.7",
        "concurrently": "^6.0.0",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0"
    }
}