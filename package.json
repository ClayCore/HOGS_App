{
    "name": "hightower-ogs-website",
    "version": "0.1.0",
    "private": true,
    "engines": {
        "node": "^15.0.0",
        "yarn": "^1.12.4"
    },
    "browserslist": [
        "since 2019-12"
    ],
    "lint-staged": {
        "*.{tsx, ts, js, scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "alias": {
        "~/*": [
            "./src/*"
        ],
        "#package/assets/*": [
            "./assets/*"
        ],
        "#package/styles": [
            "./src/website/styles/*"
        ]
    },
    "scripts": {
        "build:client": "parcel build src/index.html --public-url / --out-dir public/",
        "build:server": "tsc",
        "start:client": "parcel --no-autoinstall src/index.html --public-url / --out-dir public/",
        "start:server": "ts-node -r tsconfig-paths/register src/server.ts",
        "start": "concurrently \"yarn start:server\" \"yarn start:client\"",
        "serve": "yarn build:server && yarn start:server",
        "heroku-postbuild": "yarn build:client"
    },
    "dependencies": {
        "axios": "^0.21.0",
        "bluebird": "^3.7.2",
        "i18next": "^19.8.4",
        "intl": "^1.2.5",
        "ionicons": "^5.2.3",
        "lodash": "^4.17.20",
        "luxon": "^1.25.0",
        "moment": "^2.29.1",
        "react": "^16.13.1",
        "react-avatar-editor": "^11.0.7",
        "react-dom": "^16.13.1",
        "react-i18next": "^11.8.5",
        "react-intl": "^3.11.0",
        "react-redux": "^7.1.3",
        "react-rellax-wrapper": "1.0.6",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-router-hash-link": "^2.4.0",
        "react-scripts": "^3.3.0",
        "react-tiny-fab": "^3.2.0",
        "react-toastify": "^5.3.2",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
        "textcomplete": "^0.18.2",
        "typescript": "^4.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0-0",
        "@babel/preset-typescript": "^7.13.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.30",
        "@fortawesome/free-solid-svg-icons": "^5.15.1",
        "@fortawesome/react-fontawesome": "^0.1.12",
        "@toast-ui/react-editor": "^2.5.0",
        "@types/luxon": "^1.25.1",
        "@types/node": "^14.14.14",
        "@types/react": "^16.9.43",
        "@types/react-avatar-editor": "^10.3.4",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.5",
        "@types/react-router": "^5.1.4",
        "@types/react-router-dom": "^5.1.3",
        "@types/react-router-hash-link": "^1.2.1",
        "@typescript-eslint/eslint-plugin": "^4.20.0",
        "@typescript-eslint/parser": "^4.20.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "babel-preset-react": "^6.24.1",
        "concurrently": "^6.0.0",
        "eslint": "^7.23.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-css-modules": "^2.11.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-react": "^7.23.1",
        "gh-pages": "^3.1.0",
        "parcel-bundler": "^1.12.4",
        "prettier": "^2.1.2",
        "sass": "^1.26.11",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0"
    }
}
