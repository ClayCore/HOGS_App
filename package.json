	"private": true,
	"engines": {
		"node": "^15.0.0",
		"yarn": "^1.12.4"
	},
	"browserslist": [
		"last 3 versions"
	],
	"lint-staged": {
		"*.{tsx, ts, js, scss}": [
			"prettier --write",
			"git add"
