{
	"name": "hightower-ogs-website",
	"version": "0.1.0",
	"private": true,
	"engines": {
		"node": "^15.0.0",
		"yarn": "^1.12.4"
	},
	"browserslist": [
		"last 3 versions"
	],
	"lint-staged": {
		"*.{tsx, ts, js, scss}": [
			"prettier --write",
			"git add"
		]
	},
	"scripts": {
		"build": "yarn build:client && yarn build:server",
		"build:client": "gulp build",
		"build:server": "gulp server",
		"start": "node server/build",
		"dev": "cross-env NODE_ENV=development concurrently -k \"gulp watch\" \"yarn start\"",
		"clean": "rimraf server/build client/build",
		"serve": "yarn start",
		"heroku-postbuild": "yarn build"
	},
	"dependencies": {
		"axios": "^0.21.1",
		"connect-livereload": "^0.6.1",
		"express": "^4.17.1",
		"jsdom": "^16.5.2",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.3"
	},
	"devDependencies": {
		"@prettier/plugin-pug": "^1.13.5",
		"@types/connect-livereload": "^0.5.31",
		"@types/express": "^4.17.11",
		"@types/jsdom": "^16.2.10",
		"@types/node": "^14.14.14",
		"@typescript-eslint/eslint-plugin": "^4.20.0",
		"@typescript-eslint/parser": "^4.20.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"babel-preset-react": "^6.24.1",
		"concurrently": "6.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^7.23.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-css-modules": "^2.11.0",
		"eslint-plugin-import": "^2.22.1",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^7.0.1",
		"gulp-dart-sass": "^1.0.2",
		"gulp-livereload": "^4.0.2",
		"gulp-plumber": "^1.2.1",
		"gulp-pug": "^4.0.1",
		"gulp-typescript": "^6.0.0-alpha.1",
		"gulp-uglify": "^3.0.2",
		"prettier": "^2.1.2",
		"rimraf": "^3.0.2"
	},
	"resolutions": {
		"gulp-pug/pug": "^3.0.2"
	}
}
